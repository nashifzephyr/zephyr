if SOC_NRF52840_QIAA

config SOC
	string
	default nRF52840_QIAA

if !HAS_DTS
config SRAM_SIZE
	default 256

config FLASH_SIZE
	default 1024
endif

config NUM_IRQS
	int
	default 46

endif # SOC_NRF52840_QIAA

